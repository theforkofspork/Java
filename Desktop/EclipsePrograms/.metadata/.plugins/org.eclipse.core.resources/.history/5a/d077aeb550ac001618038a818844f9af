package com.Timer;

import java.awt.Canvas;
import java.awt.Color;

public class Timer extends Canvas{

	private static final long serialVersionUID = 5786145012149474206L;
	final int frameWidth = 500, frameHeight = frameWidth/12*9;
	double currentTime = 0.0;

	public Timer(){
		new Window(frameWidth,frameHeight,"Frame",Color.BLACK, this);
	}
	public static void main(String[] args)
	{
		new Timer();
	}
	
}
